msgid "WARNING: %s not found. obrun may not work correctly."
msgstr ""
"WARNUNG: %s nicht gefunden. Vielleicht wird obrun nicht korrekt "
"funktionieren."

msgid "Run…"
msgstr "Ausführen…"

msgid "Enter a program to run"
msgstr "Eingeben ein Programm, um es auszuführen"

msgid "Select a program to run out of the list"
msgstr "Wahlen ein Programm aus der Liste, um es auszuführen"

msgid "Available programs"
msgstr "Verfügbare Programme"

msgid "%s [action] [option]"
msgstr "%s [Aktion] [Option]"

msgid ""
"actions:\n"
"list\n"
"  choose from a list of the programs with desktop entries\n"
"path\n"
"  choose from a list of programs available in your path\n"
"simple\n"
"  produces a simple run dialog\n"
msgstr ""
"Aktionen:\n"
"l\n"
"  stellt eine Liste von Desktop-Programmen vor\n"
"p\n"
"  stellt eine Liste von Programmen in ihrem Weg vor\n"
"s\n"
"  erzeugt ein einfaches Ausführung-Dialog\n"

msgid ""
"options:\n"
"-h         Show this help\n"
"-v         Display the version\n"
msgstr ""
"Optionen:\n"
"-h         Zeigt diese Hilfe\n"
"-v         Zeigt die Version\n"
