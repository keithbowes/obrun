msgid "WARNING: %s not found. obrun may not work correctly."
msgstr ""
"ADVERTENCIA: %s no está encontrado. Es posible que obrun no funcionará "
"correctamente."

msgid "Run…"
msgstr "Ejecutar…"

msgid "Enter a program to run"
msgstr "Entrar un programa a ejecutar"

msgid "Select a program to run out of the list"
msgstr "Elegir un programa de la lista a ejecutar"

msgid "Available programs"
msgstr "Programas disponibles"

msgid "%s [action] [option]"
msgstr "%s [acción] [opción]"

msgid ""
"actions:\n"
"list\n"
"  choose from a list of the programs with desktop entries\n"
"path\n"
"  choose from a list of programs available in your path\n"
"simple\n"
"  produces a simple run dialog\n"
msgstr ""
"acciones:\n"
"l\n"
"  presenta una lista de programas de escritorio\n"
"p\n"
"  presenta una lista de programas en su vía\n"
"s\n"
"  presenta un cuadro de diálogo de ejecución\n"

msgid ""
"options:\n"
"-h         Show this help\n"
"-v         Display the version\n"
msgstr ""
"opciones:\n"
"-h        Muestra esta mensaje de ayuda\n"
"-v        Muestra la versión\n"
